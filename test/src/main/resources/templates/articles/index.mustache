{{>layouts/header}}

<table class="table">
    <thead>
<tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody class="table-group-divider">
    {{#articleList}}  <!--이게 리스트인 경우 알아서 반복을 돌림-->
    <tr>
        <th scope="row">{{id}}</th>
        <td><a href="/articles/{{id}}">{{title}}</a></td>
        <td>{{content}}</td>
    </tr>
    {{/articleList}}  <!--여기까지-->
    </tbody>
</table>

<button type="button" class="btn btn-primary" onclick = "location.href = '/articles/new' ">New Article</button>

{{>layouts/footer}}